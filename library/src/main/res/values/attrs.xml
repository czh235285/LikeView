<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="defaultColor" format="color"></attr>
    <attr name="cirRadius" format="dimension"></attr>
    <attr name="cycleTime" format="integer"></attr>

    <declare-styleable name="LikeView">
        <attr name="defaultColor" />
        <attr name="checkedColor" />
        <attr name="cirRadius" />
        <attr name="durationTime" />
        <attr name="unlike_style" />

    </declare-styleable>
    <attr name="unlike_style" format="enum">
        <enum name="NORMAL" value="0" />
        <enum name="SHRINK" value="1" />
        <enum name="BROKEN" value="2" />
    </attr>

</resources>